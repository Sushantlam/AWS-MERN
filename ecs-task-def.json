{
    "family": "backend-task",
    "networkMode": "awsvpc",
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "1024",
    "memory": "3072",
    "executionRoleArn": "arn:aws:iam::509219055572:role/ecsTaskExecutionRole",
    "containerDefinitions": [
      {
        "name": "backend",
        "image": "nginx:latest",
        "essential": true,
        "portMappings": [
          {
            "containerPort": 5000,
            "protocol": "tcp"
          }
        ]
      }
    ]
  }
  